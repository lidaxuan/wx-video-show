.my-box {
    min-height: 100%;
}

.profile-box {
    position: relative;
    top: -1px;
    padding: 212rpx 30rpx 30rpx;
    color: #333;
    background-color: #fff;
}

.profile-box::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 134rpx;
    background: linear-gradient(270deg, rgba(254, 115, 88, 1) 0%, rgba(235, 70, 84, 1) 100%);
}

.head {
    position: absolute;
    left: 20rpx;
    top: 67rpx;
    height: 150rpx;
    width: 150rpx;
    background-color: #fff;
    border-radius: 100%;
}

.head image {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    height: 140rpx;
    width: 140rpx;
    border-radius: 100%;
}

.btn-set {
    position: absolute;
    top: 82rpx;
    right: 52rpx;
    height: 36rpx;
    width: 36rpx;
}

.btn-set image {
    height: 100%;
    width: 100%;
}

.btn-follow {
    position: absolute;
    top: 194rpx;
    right: 30rpx;
    height: 60rpx;
    width: 140rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    background: linear-gradient(222deg, rgba(253, 108, 88, 1) 0%, rgba(236, 72, 84, 1) 100%);
    border-radius: 30rpx;
}

.name {
    padding-top: 16rpx;
    padding-right: 170rpx;
    font-size: 32rpx;
}

.info {
    padding-top: 16rpx;
}

.info text {
    position: relative;
    display: inline-block;
    height: 36rpx;
    padding-left: 40rpx;
    margin-right: 36rpx;
    line-height: 36rpx;
    font-size: 24rpx;
}

.info text::before {
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    height: 30rpx;
    width: 30rpx;
    background-position: center center;
    background-size: 100% 100%;
    border-radius: 100%;
}

.info .auth.type1::before {
    background-image: url(data:image/png;base64,);
}

.info .auth.type2::before {
    background-image: url(data:image/png;base64,);
}

.info .auth.type3::before {
    background-image: url(data:image/png;base64,);
}

.info .addr.type1::before {
    background-image: url(data:image/png;base64,);
}

.info .addr.type2::before {
    background-image: url(data:image/png;base64,);
}

.info .addr.unknown {
    color: #999;
}

.intro {
    position: relative;
    padding-top: 30rpx;
    margin-top: 50rpx;
    font-size: 26rpx;
    color: #999;
}

.intro::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    border-top: 1px solid #eee;
    transform: scaleY(0.5);
}

.achieve {
    padding-top: 90rpx;
    text-align: center;
}

.achieve .item {
    position: relative;
    display: inline-block;
    width: 33.33%;
}

.achieve .item::after {
    content: '';
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%) scaleX(0.5);
    height: 20rpx;
    border-right: 1px solid #bbb;
}

.achieve .item:last-child:after {
    border-right: none;
}

.achieve .item text {
    display: block;
}

.achieve .value {
    font-size: 36rpx;
}

.achieve .type {
    padding-top: 10rpx;
    font-size: 24rpx;
    color: #bbb;
}

.tab-box {
    height: 80rpx;
    padding: 0rpx 0;
    text-align: center;
    display: flex;
    justify-content: center;
    background-color: #fff;
}

.tab-box::before {
    content: '';
    display: block;
    height: 14rpx;
    background-color: #f7f7f7;
}

.tab {
    background-color: #fff;
    transition: all 0.24s;
}

.tab-box.fixed {
    display: none;
    z-index: 1;
    position: fixed;
    left: 0;
    right: 0;
    margin-top: 0;
    width: 100%;
    justify-content: center;
    background-color:#fff;
}

.tab-box.fixed.show {
    display: flex;
}

.tab-box .tab {
    display: inline-block;
    height: 100%;
    width: 33.33%;
    /* flex-grow:1; */
    line-height: 80rpx;
    font-size: 26rpx;
    color: #999;
}

.tab-box .tab.active {
    position: relative;
    color: #333;
}

.tab-box .tab.active::after {
    content: '';
    position: absolute;
    left: 50%;
    bottom: 6rpx;
    transform: translateX(-50%);
    height: 4rpx;
    width: 20rpx;
    background-color: #333;
    border-radius: 2rpx;
}

.list-box .empty {
    padding: 80rpx 0 100rpx;
    text-align: center;
    font-size: 30rpx;
    color: #999;
}

.list-box .empty image {
    display: block;
    height: 220rpx;
    width: 220rpx;
    margin: auto;
}

.list-box .empty text {
    display: block;
    padding-top: 40rpx;
}

.list {
    display: none;
    background-color: #fff;
}

.list.show {
    display: block;
}

.like-box-bg {
    display: none;
    z-index: 2;
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.5);
    transition: opacity 0.24s;
}

.like-box-bg.show {
    display: block;
    opacity: 0;
    will-change: opacity;
}

.like-box-bg.show.opacity {
    opacity: 1;
}

.like-box {
    display: none;
    z-index: 3;
    position: fixed;
    left: 50%;
    top: 50%;
    height: 504rpx;
    width: 520rpx;
    margin: -252rpx 0 0 -250rpx;
    background-color: #fff;
    border-radius: 20rpx;
    transform: scale(0);
}

.like-box.show {
    display: block;
    will-change: transform;
}

.like-box.show.zoom-in {
    transform: scale(1);
    animation: zoom-in 0.3s;
}

.like-box.show.zoom-out {
    transform: scale(0);
    animation: zoom-out 0.3s;
}

.like-box .like {
    display: block;
    height: 242rpx;
    width: 242rpx;
    margin: 60rpx auto;
}

.like-box .close {
    position: absolute;
    left: 50%;
    bottom: -140rpx;
    transform: translateX(-50%);
    height: 80rpx;
    width: 80rpx;
}

.like-box view {
    text-align: center;
    font-size: 32rpx;
    color: #333;
}

.like-box text {
    color: #ec4754;
}

@keyframes zoom-in {
    0% {
        transform: scale(0);
    }

    60% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes zoom-out {
    0% {
        transform: scale(1);
    }

    20% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(0);
    }
}

.my-invite {
    display: flex;
    align-items: center;
    height: 96rpx;
    padding: 0 22rpx;
    margin-top: 62rpx;
    font-size: 28rpx;
    color: #ef4e55;
    background-color: #fff0f0;
    border-radius: 8rpx;
}

.my-invite .icon-friend {
    height: 34rpx;
    width: 46rpx;
    margin-right: 26rpx;
}

.my-invite .r {
    padding-right: 20rpx;
    margin-left: auto;
    font-size: 40rpx;
    background: url(data:image/png;base64,) no-repeat right center/12rpx 18rpx;
}

.my-invite .r text {
    display: inline-block;
    vertical-align: middle;
}

.my-invite .r text:last-child {
    padding-left: 6rpx;
    font-size: 22rpx;
}
