.search {
    z-index: 9999;
    padding-bottom: 16rpx;
    background: linear-gradient(270deg, rgba(254, 115, 88, 1) 0%, rgba(235, 70, 84, 1) 100%);
}

.inner {
    position: relative;
    display: flex;
    height: 60rpx;
}

.btn {
    height: 60rpx;
    width: 60rpx;
    margin-right: 10rpx;
    text-indent: -9999px;
    background-repeat: no-repeat;
    background-position: center center;
}

